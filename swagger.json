{
    "openapi": "3.0.0",
    "info": {
        "title": "API Virtumed",
        "description": "API para médicos se contactarem com seus pacientes virtualmente.",
        "license": {
            "name": "MIT",
            "url": "https://opensource.org/licenses/MIT"
        },
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "http://localhost:3000/api",
            "description": "API"
        }
    ],
    "paths": {
        "/create-doctor": {
            "post": {
                "summary": "Cadastrar um doctor",
                "description": "Rota responsável por cadastrar um doctor",
                "tags": ["Doctor"],
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/Doctors"
                            },
                            "examples": {
                                "Paletas": {
                                    "value": {
                                        "crm": "Johnnie Walker Double Black",
                                        "cpf": "Whisky Escocês",
                                        "": "",
                                        "": "",
                                        "": ""
                                    }
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "400": {
                        "description": "Bad Request"
                    },
                    "201": {
                        "description": "Created",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "$ref": "#/components/schemas/Doctors"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/all-doctors": {
            "get": {
                "summary": "Lista de todas os doutores",
                "description": "Rota responsável por listar todas os doutores",
                "tags": ["Doctors"],
                "responses": {
                    "404": {
                        "description": "Not Found"
                    },
                    "200": {
                        "description": "OK",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/Doctors"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/one-doctor/{id}": {
            "get": {
                "summary": "Busca uma doctor pelo ID",
                "description": "Rota responsável por buscar um doctor pelo ID",
                "tags": ["Doctors"],
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "Id do doctor para a busca",
                        "required": true
                    }
                ],
                "responses": {
                    "400": {
                        "description": "Bad Request"
                    },
                    "200": {
                        "description": "OK",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "$ref": "#/components/schemas/Doctors"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/update-doctor/{id}": {
            "put": {
                "summary": "Editar um doctor pelo ID",
                "description": "Rota responsável por editar um doctor pelo ID",
                "tags": ["Doctors"],
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "Id do doctor para a busca",
                        "required": true
                    }
                ],
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/Doctors"
                            },
                            "examples": {
                                "Paletas": {
                                    "value": {
                                        "": "",
                                        "": "",
                                        "": "",
                                        "": ""
                                    }
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "400": {
                        "description": "Bad Request"
                    },
                    "200": {
                        "description": "OK",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "$ref": "#/components/schemas/Doctors"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/delete-doctor/{id}": {
            "delete": {
                "summary": "Apagar uma doctor pelo ID",
                "description": "Rota responsável por apagar uma doctor pelo ID",
                "tags": ["Doctors"],
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "Id do doctor para deleção",
                        "required": true
                    }
                ],
                "responses": {
                    "400": {
                        "description": "Bad Request"
                    },
                    "200": {
                        "description": "OK"
                    }
                }
            }
        },
        "/all-pacient": {
            "get": {
                "summary": "Lista de todos os itens do pacient",
                "description": "Rota responsável por listar todos os pacient",
                "tags": ["Pacient"],
                "responses": {
                    "404": {
                        "description": "Not Found"
                    },
                    "200": {
                        "description": "OK",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/Pacient"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/create-pacient": {
            "post": {
                "summary": "Criar o pacient",
                "description": "Rota responsável por criar o pacient",
                "tags": ["Pacient"],
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/Pacient"
                            },
                            "examples": {
                                "Pacient": {
                                    "value": [
                                        {
                                            "paletaId": "624230fdcd38269205c82197",
                                            "quantidade": 5
                                        },
                                        {
                                            "paletaId": "62423c1e618f4a4738a5ee75",
                                            "quantidade": 2
                                        }
                                    ]
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "400": {
                        "description": "Bad Request"
                    },
                    "201": {
                        "description": "Created",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/Pacient"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/delete-pacient": {
            "delete": {
                "summary": "Apagar todos os pacient",
                "description": "Rota responsável por apagar todos os pacient",
                "tags": ["Pacient"],
                "responses": {
                    "400": {
                        "description": "Bad Request"
                    },
                    "200": {
                        "description": "OK"
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "Pacient": {
                "type": "object",
                "properties": {
                    "cpf": {
                        "type": "string"
                    },
                    "cep": {
                        "type": "string"
                    },
                    "user": {
                        "type": "string"
                    },
                    "created_at": {
                        "type": "string"
                    },
                    "updated_at": {
                        "type": "string"
                    }
                }
            },
            "Doctor": {
                "type": "object",
                "properties": {
                    "crm": {
                        "type": "string"
                    },
                    "cpf": {
                        "type": "number"
                    },
                    "cep": {
                        "type": "number"
                    },
                    "created_at": {
                        "type": "string"
                    },
                    "updated_at": {
                        "type": "string"
                    }
                }
            }
        }
    }
}
